{
  "name": "rn-expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf node_modules android ios yarn.lock",
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build:android": "expo prebuild --platform android && cd android && ./gradlew assembleRelease",
    "build:ios-sim": "expo prebuild --platform ios && cd ios && xcodebuild -workspace mflixexpo.xcworkspace -scheme mflixexpo -configuration Release -sdk iphonesimulator -destination 'platform=iOS Simulator,name=iPhone 16 Pro'",
    "build:ios-device": "expo prebuild --platform ios && cd ios && xcodebuild -workspace mflixexpo.xcworkspace -scheme mflixexpo -configuration Release -sdk iphoneos -archivePath build/mflixexpo.xcarchive archive",
    "build:ios": "npm run build:ios-sim",
    "build:release": "npm run build:android && npm run build:ios",
    "install:android": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "install:android-all": "for device in $(adb devices | grep device | cut -f1); do adb -s $device install android/app/build/outputs/apk/release/app-release.apk; done",
    "install:ios-sim": "xcrun simctl install booted ios/build/Release-iphonesimulator/mflixexpo.app"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@dittolive/ditto": "^4.12.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "1.7.6",
    "expo": "^53.0.20",
    "expo-asset": "~11.1.7",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "@expo/config-plugins": "~10.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.1.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
